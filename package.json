{
  "name": "@jolacdev/kcd-localization-toolkit-cli",
  "version": "1.0.0",
  "description": "Kingdom Come: Deliverance - Localization Toolkit (CLI)",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "clean": "rimraf dist",
    "build": "npm run clean && tsc -p tsconfig.build.json",
    "dev": "node --experimental-transform-types --loader ts-node src/index.ts",
    "start": "echo \"Error: no production run script specified\" && exit 1",
    "lint": "eslint --fix",
    "lint:no-fix": "eslint",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:gui": "vitest --ui"
  },
  "keywords": [
    "cli",
    "modding",
    "localization",
    "toolkit",
    "kcd",
    "kingdom-come-deliverance"
  ],
  "author": "Jose L",
  "license": "ISC",
  "devDependencies": {
    "@eslint/js": "9.32.0",
    "@types/i18next": "12.1.0",
    "@types/i18next-fs-backend": "1.1.5",
    "@types/jest": "30.0.0",
    "@types/node": "24.2.0",
    "@types/prompts": "2.4.9",
    "@typescript-eslint/parser": "8.39.0",
    "@vitest/ui": "3.2.4",
    "eslint": "9.32.0",
    "eslint-config-prettier": "10.1.8",
    "eslint-plugin-perfectionist": "4.15.0",
    "eslint-plugin-prettier": "5.5.4",
    "globals": "16.3.0",
    "prettier": "3.6.2",
    "rimraf": "6.0.1",
    "ts-node": "10.9.2",
    "typescript": "5.9.2",
    "typescript-eslint": "8.39.0",
    "vitest": "3.2.4"
  },
  "dependencies": {
    "i18next": "25.3.2",
    "i18next-fs-backend": "2.6.0",
    "prompts": "2.4.2"
  }
}
